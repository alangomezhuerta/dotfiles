# This file should be copied in your home directory and renamed as .env
# then you should call (source) it from your .bashrc, .profile, .zshrc
# files



# Update this variable with the path of your "do" directory
export DO_DIR=/do

# Environment variables for vagrant under WSL
export VAGRANT_WSL_ENABLE_WINDOWS_ACCESS="1"
export PATH="$PATH:/mnt/c/Program Files/Oracle/VirtualBox"
export PATH="$PATH:/mnt/c/Windows/System32"
